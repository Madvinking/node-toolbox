{
  "name": "@logzio-node-toolbox/utils",
  "version": "0.0.2",
  "description": "collection of nice utils",
  "main": "src/index.cjs",
  "exports": {
    "import": "./src/index.js",
    "require": ".src/index.cjs"
  },
  "types": "src/index.d.ts",
  "type": "module",
  "license": "MIT",
  "author": "Nir winkler <madvinking@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/logzio/node-toolbox.git",
    "directory": "packages/utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "upkg": "ncu --packageFile ./package.json",
    "compile": "rollup src/index.js --format cjs --file src/index.cjs"
  }
}
